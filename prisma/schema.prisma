generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model JadwalMakan {
  jadwalId    Int   @id @default(autoincrement())
  jadwalMakan String
  waktu       DateTime
  user        User     @relation(fields: [userId], references: [userUID])
  userId      String
}

model User {
  userUID       String        @id
  identifier    String        @unique
  name          String
  jadwalMakanan JadwalMakan[]
}
